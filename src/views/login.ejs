<%- include('./partials/header') %> 

<link rel="stylesheet" href="/stylesheets/login-grid.css">

<form class="contenedor login-form" action="/users/auth" method="POST">
    <h1 class="titulo">Ingrese sus Datos</h1>
        <div class="label1">
            <label for="">Email:</label>
        </div>
        <div class="input1">
            <input type="email" name="email" id="email">
        </div>
        <div class="label2">
            <label for="">Password</label>
        </div>
        <div class="input2">
            <input type="password" name="password" id="password">
        </div>
        <div class="checkbxlbl">
            <label for="" class="recordarme">Recordarme</label>
        </div>
        <div class="checkbxinp">
            <input type="checkbox" class="rec">
        </div>
        <div class="login">
            <button type="submit" class="ingresar">Ingresar</button>
        </div>
</form>


<script src="/scripts/formLogin.js"></script>
<%- include('./partials/footer') %> 